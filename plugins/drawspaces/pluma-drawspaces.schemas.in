<mateconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/enable</key>
      <applyto>/apps/pluma/plugins/drawspaces/enable</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Show White Spaces</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_tabs</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_tabs</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Draw Tabs</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_spaces</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_spaces</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Draw Spaces</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_newline</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_newline</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
	<short>Draw New lines</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_nbsp</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_nbsp</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
	<short>Draw Non-Breaking Spaces</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_leading</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_leading</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Draw Leading Spaces</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_text</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_text</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Draw Spaces in Text</short>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/pluma/plugins/drawspaces/draw_trailing</key>
      <applyto>/apps/pluma/plugins/drawspaces/draw_trailing</applyto>
      <owner>pluma</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
	<short>Draw Trailing Spaces</short>
      </locale>
    </schema>
  </schemalist>
</mateconfschemafile>
